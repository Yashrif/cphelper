{
  "name": "cphelper",
  "productName": "cphelper v0.1.0",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "main": "core/main.ts",
  "scripts": {
    "start:web": "vite",
    "build:web": "tsc && vite build",
    "preview": "vite preview",
    "build:desktop": "tsc -p core/tsconfig.json",
    "start:desktop": "npm run build:desktop && electron build/main.js",
    "migrate": "cd core && npx prisma migrate dev --name init",
    "studio": "cd core && npx prisma studio",
    "build": "electron-builder --x64",
    "publish": "electron-builder --x64 --win -p always",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.3.2",
    "electron": "^23.0.0",
    "framer-motion": "^9.0.2",
    "kute.js": "^2.2.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "redux-logger": "^3.0.6",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
